a = ("Ákos", "Bence", "Marci", "Karcsi", "Kevin", "Feri", "Zsuzsi", "Vikrot", "Merkel", "Gergő")


for i in ["Ákos", "Bence", "Marci", "Karcsi", "Kevin", "Feri", "Zsuzsi", "Vikrot", "Merkel", "Gergő"]:
    print("Szia "+ i +", sok szeretettel várlak a házibulimban")

print()# sortör

print(len(a))

print()# sortör


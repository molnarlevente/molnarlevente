for g in ["Ákos", "Bence", "Marci", "Karcsi", "Kevin", "Feri", "Zsuzsi", "Vikrot", "Merkel", "Gergő"]:
    print("Szia " + g + ", hogy vagy?")


print()# sortör

ú = 2
ó = 10

print(ú * ó)        #20
print(ú ** ó)       #20
print(ó / ú)        #5.0
print(ó // ú)       #5
print(ú % ó)        #2
print(ó + ú)        #12
print(ú - ó)        #-8

print()# sortör

